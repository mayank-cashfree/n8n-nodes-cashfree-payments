{
  "name": "@cashfreepayments/n8n-nodes-cashfree-payments",
  "version": "0.1.1",
  "description": "n8n community node for Cashfree Payments API integration - supports orders, payment links, refunds, and cashgrams",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "cashfree",
    "payments",
    "payment-gateway",
    "payment-links",
    "refunds",
    "cashgram",
    "india"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/cashfree/n8n-nodes-cashfree-payments",
  "author": {
    "name": "Cashfree Payments",
    "url": "https://www.cashfree.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cashfree/n8n-nodes-cashfree-payments.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:post",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint nodes credentials --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CashfreeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CashfreePayments/CashfreePayments.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "gulp": "^4.0.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
