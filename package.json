{
  "name": "n8n-nodes-cashfree-payments",
  "version": "1.0.0",
  "description": "n8n community node for Cashfree Payments API integration - supports orders, payment links, refunds, and cashgrams",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "cashfree",
    "payments",
    "payment-gateway",
    "payment-links",
    "refunds",
    "cashgram",
    "india"
  ],
  "license": "MIT",
  "homepage": "https://github.com/cashfree-tech/n8n-nodes-cashfree-payments",
  "author": {
    "name": "Cashfree Payments",
    "email": "aditya.chauhan@cashfree.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cashfree-tech/n8n-nodes-cashfree-payments.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=7.18"
  },
  "packageManager": "pnpm@7.18.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CashfreeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CashfreePayments/CashfreePayments.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
